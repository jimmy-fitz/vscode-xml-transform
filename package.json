{
	"name": "vscode-xml-transform",
	"displayName": "vscode-xml-transform",
	"description": "XML-XSL Transformation Extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.xml-transform"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.xml-transform",
				"title": "Xml Transform: Transform"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.xml-transform",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"arraybuffer-to-string": "^1.0.2",
		"read-file": "^0.2.0",
		"xslt-processor": "^0.11.1"
	}
}
